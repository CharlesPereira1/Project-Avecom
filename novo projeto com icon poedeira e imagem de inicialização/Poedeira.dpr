{$R *.RES}

begin
  Hwnd:= FindWindow('TApplication', 'Manejo de Aves');
  if (Hwnd = 0) then {Se Hwnd <> 0, uma instância da aplicação já está aberta}
    begin
      FSplash := TFSplash.Create(Application);
      FSplash.Show;
      FSplash.Update;


      Application.Initialize;
      Application.Title := 'Poedeiras';
  Application.HelpFile := '';
  Application.CreateForm(TFMenu, FMenu);
  Application.CreateForm(TFConfiguracoes, FConfiguracoes);
  Application.CreateForm(TFDistribuicaoBox, FDistribuicaoBox);
  Application.CreateForm(Tfvisualizado, fvisualizado);
  FSplash.Destroy;
      Application.Run;
    end
  else
    begin
      if not isWindowVisible(Hwnd) then
        begin
          PostMessage (Hwnd, wm_User, 0,0) ;
          ShowWindow(Hwnd, SW_MAXIMIZE);
        end;
      SetForegroundWindow(Hwnd);
    end;
end.
